    border: none;
    -moz-box-shadow:none;
    -webkit-box-shadow:none;
    box-shadow:none;
    text-shadow:none;
}
.FS2_SystemNav_btnPC{
  cursor: pointer;
    width:94%;
    margin:10px auto;
